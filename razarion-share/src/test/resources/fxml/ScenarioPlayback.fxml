            <Label fx:id="timeLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            </children>
        </GridPane>
        <VBox prefHeight="259.0" prefWidth="155.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="134.0" text="Sync item id" />
                        <TextField onKeyReleased="#onSyncItemTextChanged" />
                    </children>
                </HBox>
                <TableView fx:id="syncItemPropertyTable" prefHeight="346.0" prefWidth="155.0">
                    <columns>
                        <TableColumn fx:id="syncItemPropertyTableNameColumn" prefWidth="66.0" text="Property" />
                        <TableColumn fx:id="syncItemPropertyTableValueColumn" prefWidth="84.0" text="Value" />
